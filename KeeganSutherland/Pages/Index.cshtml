@model IndexModel
@{
    ViewData["Title"] = "Home";

    DateTime dateOfBirth = new DateTime(1994, 8, 23);
    DateTime dateOfDevCasually = new DateTime(2013, 8, 19);
    DateTime dateOfDevProfessionally = new DateTime(2016, 5, 9);


    int age = (int.Parse(DateTime.Now.ToString("yyyyMMdd")) - int.Parse(dateOfBirth.ToString("yyyyMMdd"))) / 10000;
    int devCasuallyYears = (int.Parse(DateTime.Now.ToString("yyyyMMdd")) - int.Parse(dateOfDevCasually.ToString("yyyyMMdd"))) / 10000;
    int devProfessionallyYears = (int.Parse(DateTime.Now.ToString("yyyyMMdd")) - int.Parse(dateOfDevProfessionally.ToString("yyyyMMdd"))) / 10000;
}

<!-- ======= Hero Section ======= -->
<section id="hero" class="d-flex flex-column justify-content-center align-items-center">
    <div class="hero-container" data-aos="fade-in">
        <h1>Keegan Sutherland</h1>
        <p>I'm a <span class="typed" data-typed-items="Full Stack .Net Web Developer, Tech Enthusiast, Runner, Husband"></span></p>
    </div>
</section><!-- End Hero -->
<main id="main">
    <!-- ======= About Section ======= -->
    <section id="about" class="about">
        <div class="container">
            <div class="section-title">
                <h2>About</h2>
                <p>Hi, I'm Keegan! I've been developing casually for about @devCasuallyYears years and professionally for about @devProfessionallyYears years.
                I am very motivated, disciplined, and organized when it comes to both my personal life and professional career. In my free time I love to run, hike, bike, swim in Lake Michigan, tinker around with my gadgets, and play video games.
                I love the outdoors, especially exploring West Michigan and all it has to offer from the towns, to the beaches, to the trails.</p>
            </div>
            <div class="row">
                <div class="col-lg-4" data-aos="fade-right">
                    <img src="~/img/profile-img.jpg" class="img-fluid" alt="">
                </div>
                <div class="col-lg-8 pt-4 pt-lg-0 content" data-aos="fade-left">
                    <h3>Full Stack .Net Web Developer.</h3>
                    <p class="font-italic">
                        I love working in the .Net stack and learning all about the newest .Net technologies. I develop with frontend, backend, and database technologies to create a seamless experience for the user.
                        I utilize AWS to keep hosting costs low and downtime minimal. I've developed websites, web applications, applications, and SOAP/REST APIs.
                    </p>
                    <div class="row">
                        <div class="col-lg-6">
                            <ul>
                                <li><i class="icofont-rounded-right"></i> <strong>Birthday: </strong>@dateOfBirth.ToString("MMM dd, yyyy")</li>
                                <li><i class="icofont-rounded-right"></i> <strong>Age: </strong>@age</li>
                                <li><i class="icofont-rounded-right"></i> <strong>Website: </strong>keegansutherland.com</li>
                                <li><i class="icofont-rounded-right"></i> <strong>City: </strong>Grand Rapids, MI</li>
                            </ul>
                        </div>
                        <div class="col-lg-6">
                            <ul>
                                <li><i class="icofont-rounded-right"></i> <strong>Degree: </strong>Bachelor of Science</li>
                                <li><i class="icofont-rounded-right"></i> <strong>Major: </strong>IS – Systems Development/Analyst</li>
                                <li><i class="icofont-rounded-right"></i> <strong>Minor: </strong>Business Administration</li>
                                <li><i class="icofont-rounded-right"></i> <strong>Freelance: </strong> Available</li>
                            </ul>
                        </div>
                    </div>
